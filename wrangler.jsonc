{
  "$schema": "node_modules/wrangler/config-schema.json",
  "main": ".open-next/worker.js",
  "name": "nextjs-on-cloudflare-workers",
  "keep_names": false,
  "dev": {
    "port": 3006
  },
  "compatibility_date": "2025-03-01",
  "compatibility_flags": ["nodejs_compat", "global_fetch_strictly_public"],
  "observability": {
    "enabled": true
  },
  "placement": {
    "mode": "smart"
  },
  "assets": {
    "directory": ".open-next/assets",
    "binding": "ASSETS"
  },
  "services": [
    {
      "binding": "APPLICATION",
      "service": "nextjs-on-cloudflare-workers"
    }
  ],
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "nextjs-on-cloudflare-workers-db",
      "database_id": "a221e191-b8da-40a3-b31e-c26b8f266942",
      "migrations_dir": "./src/db/migrations"
    }
  ],
  "r2_buckets": [
    {
      "binding": "STORAGE",
      "bucket_name": "nextjs-on-cloudflare-workers-storage"
    }
  ],
  "kv_namespaces": [
    {
      "binding": "CACHE",
      "id": "80bfc3d073974310b06f397b0baa0e3a"
    }
  ]
}
